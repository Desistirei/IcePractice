<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>de.kontux</groupId>
  <artifactId>IcePracticePlugin</artifactId>
  <version>0.6</version>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.4.1</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <minimizeJar>false</minimizeJar>
          <artifactSet>
            <excludes>
              <exclude>com.comphenix.protocol</exclude>
              <exclude>com.comphenix.executors</exclude>
              <exclude>com.comphenix.net</exclude>
              <exclude>cglib:*</exclude>
            </excludes>
          </artifactSet>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <repositories>
    <repository>
      <id>spigot-repo</id>
      <url>https://hub.spigotmc.org/nexus/content/repositories/snapshots/</url>
    </repository>
    <repository>
      <id>jitpack.io</id>
      <url>https://jitpack.io</url>
    </repository>
    <repository>
      <id>dmulloy2-repo</id>
      <url>https://repo.dmulloy2.net/nexus/repository/public/</url>
    </repository>
    <repository>
      <id>That one repository that no one asked for</id>
      <url>https://r1.jgamingz.ml</url>
    </repository>
  </repositories>
  <dependencies>
    <dependency>
      <groupId>dev.lugami</groupId>
      <artifactId>BloomSpigot</artifactId>
      <version>1.8.8-R0.1-SNAPSHOT</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>com.comphenix.protocol</groupId>
      <artifactId>ProtocolLib</artifactId>
      <version>4.5.1</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.github.azbh111</groupId>
      <artifactId>craftbukkit-1.7.10</artifactId>
      <version>R</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>com.github.azbh111</groupId>
      <artifactId>craftbukkit-1.8.8</artifactId>
      <version>R</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>com.github.azbh111</groupId>
      <artifactId>craftbukkit-1.12.2</artifactId>
      <version>R</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>com.github.azbh111</groupId>
      <artifactId>craftbukkit-1.13.2</artifactId>
      <version>R</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>com.github.azbh111</groupId>
      <artifactId>craftbukkit-1.14.3</artifactId>
      <version>R</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>org.craftbukkit</groupId>
      <artifactId>bukkit</artifactId>
      <version>1.15-R0.1-SNAPSHOT</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/libs/Bukkit-1.15.jar</systemPath>
    </dependency>
  </dependencies>
  <properties>
    <maven.compiler.source>1.8</maven.compiler.source>
    <maven.compiler.target>1.8</maven.compiler.target>
  </properties>
</project>
